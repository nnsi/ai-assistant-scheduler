# Frontend Environment Variables
# Copy this file to .env.local and fill in the values

# API URL (optional, defaults to /api which uses Vite proxy)
# VITE_API_URL=http://localhost:8787/api

# Google OAuth Client ID (required for authentication)
# Must match the backend's GOOGLE_CLIENT_ID
VITE_GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com

# 開発環境認証バイパス: "true" でGoogle認証をスキップしてテストユーザーでログイン可能
# バックエンドの ENABLE_DEV_AUTH と合わせて設定
VITE_ENABLE_DEV_AUTH=true
